<?xml version="1.0" encoding="UTF-8"?>
<suite name="all test case" verbose="1" parallel="tests" thread-count="3" configfailurepolicy="continue">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="http">
        <packages>
            <package name="autotest.http.*" />
        </packages>
    </test>
    <test name="ui">
        <packages>
            <package name="autotest.ui.*" />
        </packages>
    </test>
    <test name="rpc">
        <packages>
            <package name="autotest.rpc.*" />
        </packages>
    </test>
</suite>