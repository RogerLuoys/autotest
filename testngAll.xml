<?xml version="1.0" encoding="UTF-8"?>
<suite name="all test case" verbose="1" parallel="tests" thread-count="3" configfailurepolicy="continue">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>

    <test name="ui">
        <packages>
            <package name="testCase.ui.*" />
        </packages>
    </test>

    <test name="http">
        <packages>
            <package name="testCase.http.*" />
        </packages>
    </test>

    <test name="rpc">
        <packages>
            <package name="testCase.rpc.*" />
        </packages>
    </test>

</suite>