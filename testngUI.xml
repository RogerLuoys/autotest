<?xml version="1.0" encoding="UTF-8"?>
<suite name="UI all test case Suite" verbose="1" parallel="tests" thread-count="2" configfailurepolicy="continue">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="flag">
        <packages>
            <package name="testCase.ui.flag.*" />
        </packages>
    </test>
    <test name="baidu">
        <packages>
            <package name="testCase.ui.baidu.*" />
        </packages>
    </test>
</suite>