<?xml version="1.0" encoding="UTF-8"?>
<suite name="HTTP all case" verbose="1" parallel="tests" thread-count="2" configfailurepolicy="continue">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="commonTestng.CommonListener" />
    </listeners>
    <test name="flag">
        <packages>
            <package name="testCase.http.flag.*" />
        </packages>
    </test>
    <test name="express">
        <packages>
            <package name="testCase.http.express.*" />
        </packages>
    </test>
</suite>