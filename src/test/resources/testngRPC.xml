<?xml version="1.0" encoding="UTF-8"?>
<suite name="RPC all case" verbose="1" parallel="tests" thread-count="1" configfailurepolicy="continue">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="commonTestng.CommonListener" />
    </listeners>

    <test name="user center">
        <packages>
            <package name="testCase.rpc.userCenter.*" />
        </packages>
    </test>

    <test name="dzb">
        <packages>
            <package name="testCase.rpc.xdzb.*" />
        </packages>
    </test>

</suite>